{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "scopeName": "text.html.tw",
    "name": "Textwire",
    "fileTypes": [
        "tw.html"
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.exception.php",
                    "match": "\b(true|false)\b"
                }
            ]
        }
    },
    "patterns": [
        {
            "include": "text.html.basic"
        }
    ]
}