{
    "name": "textwire",
    "displayName": "Textwire",
    "description": "Textwire syntax highlight support",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.87.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/textwire.png",
    "main": "./extension.js",
    "contributes": {
        "languages": [
            {
                "id": "textwire",
                "aliases": [
                    "Textwire",
                    "textwire"
                ],
                "extensions": [
                    ".tw.html"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "images/textwire.png",
                    "dark": "images/textwire.png"
                }
            }
        ],
        "grammars": [
            {
                "language": "textwire",
                "scopeName": "text.html.tw",
                "path": "./syntaxes/textwire.tmLanguage.json"
            }
        ]
    }
}
